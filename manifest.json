{
    "name": "Paint Warfare",
    "description": "A Paint Warfare themed Opera GX Mod by NotWhopper! Join the Discord Server for Paint Warfare! https://discord.gg/paint-warfare-515461022677729292",
    "developer":
    {
        "name": "NotWhopper"
    },
    "icons":
    {
        "512": "pw_orange.png"
    },
    "manifest_version": 3,
    "mod":
    {
        "license": "Thanks.txt",
        "payload":
        {
            "background_music":
            [
                "music/Combination.mp3"
            ],
            "browser_sounds":
            {
                "CLICK":
                [
                    "sound/Click.mp3"
                ],
                "HOVER":
                [
                    "sound/Hover.wav"
                ],
                "HOVER_UP":
                [
                    "sound/Hover.wav"
                ],
                "IMPORTANT_CLICK":
                [
                    "sound/Click.mp3"
                ],
                "SWITCH_TOGGLE":
                [
                    "sound/Click2.ogg"
                ],
                "TAB_CLOSE":
                [
                    "sound/Cancel.mp3"
                ],
                "TAB_INSERT":
                [
                    "sound/Confirm.mp3"
                ],
                "TAB_SLASH":
                [
                    "sound/sniper.ogg"
                ]
            },
            "page_styles":
            [
                {
                    "css":
                    [
                        "webmodding/opera.css"
                    ],
                    "matches":
                    [
                        "https://*.opera.com/*"
                    ]
                }
            ],
            "shaders":
            {
                "Matrix green":
                {
                    "path": "shader/matrix.txt"
                },
                "Wave":
                {
                    "animation":
                    {
                        "duration": 60,
                        "steps": 3600
                    },
                    "path": "shader/wave.txt"
                }
            },
            "keyboard_sounds":
            {
                "TYPING_BACKSPACE":
                [
                    "keyboard/Old_Rifle.mp3"
                ],
                "TYPING_ENTER":
                [
                    "keyboard/Click3.mp3"
                ],
                "TYPING_LETTER":
                [
                    "keyboard/Rifle.mp3"
                ],
                "TYPING_SPACE":
                [
                    "keyboard/Pop.wav"
                ]
            },"theme":
            {
                "dark":
                {
                    "gx_accent":
                    {
                        "h": 39,
                        "s": 100,
                        "l": 50
                    },
                    "gx_secondary_base":
                    {
                        "h": 120,
                        "s": 37,
                        "l": 16
                    }
                },
                "light":
                {
                    "gx_accent":
                    {
                        "h": 120,
                        "s": 80,
                        "l": 40
                    },
                    "gx_secondary_base":
                    {
                        "h": 39,
                        "s": 100,
                        "l": 88
                    }
                }
            },
            "wallpaper":
            {
                "dark":
                {   
                    "image": "wallpaper/video.webm",
					"first_frame": "wallpaper/first_frame.jpg",
                    "text_color": "#FFFFFF",
                    "text_shadow": "#757575"
                },
                "light":
                {
                    "image": "wallpaper/video.webm",
					"first_frame": "wallpaper/first_frame.jpg",
                    "text_color": "#FFFFFF",
                    "text_shadow": "#0B000E"
                }
            }
        },
        "schema_version": 1
    },
    "version": "1.1"
}
